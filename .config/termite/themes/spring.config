[options]
    resize_grip = false
    scroll_on_output = false
    scroll_on_keystroke = true
    audible_bell = true
    visible_bell = false
    mouse_autohide = true
    allow_bold = true
    dynamic_title = true
    urgent_on_bell = true
    clickable_url = true
    font = Roboto Mono for Powerline  12
    scrollback_lines = 1000
    search_wrap = true
    #icon_name = terminal
    geometry = 950x550

    # "system", "on" or "off"
    cursor_blink = system

    # "block", "underline" or "ibeam"
    cursor_shape = underline

    # $BROWSER is used by default
    browser = firefox

    # vte-256color has the correct terminfo, but some applications (vim, weechat)
    # still need patches for mouse/title support
    term = screen-256color


[colors]

# special
foreground      = #b4b394
foreground_bold = #b4b394
cursor          = #b4b394
background      = #171714

# black
color0  = #16150f
color8  = #3d4635

# red
color1  = #c74c2e
color9  = #ea8357

# green
color2  = #645e1e
color10 = #c1cb3d

# yellow
color3  = #b36316
color11 = #e8d550

# blue
color4  = #336276
color12 = #98c0c0

# magenta
color5  = #764862
color13 = #c28084

# cyan
color6  = #324f4d
color14 = #73b7ae

# white
color7  = #726b51
color15 = #b2c1a6

